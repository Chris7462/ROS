<package>
  <name>lane_marker_plugin</name>
  <version>0.10.3</version>
  <description>Adds lane markers as a display type in ROS</description>
  <maintainer email="shengsong.ding@aptiv.com">Shengsong Ding</maintainer>
  <license>Aptiv PLC. All Rights Reserved.</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>lane_marker_messages</build_depend>
  <build_depend>qtbase5-dev</build_depend>
  <build_depend>rviz</build_depend>

  <run_depend>libqt5-core</run_depend>
  <run_depend>libqt5-gui</run_depend>
  <run_depend>libqt5-widgets</run_depend>
  <run_depend>rviz</run_depend>

  <export>
      <rviz plugin="${prefix}/plugin_description.xml"/>
  </export>
</package>
